   "source": [
    "def getMinimumCost(k, c):\n",
    "    c.sort(reverse=True)\n",
    "    return sum(x*(i//k+1) for i, x in enumerate(c))\n",
    "\n",
    "print(getMinimumCost(3, [1, 2, 3, 4]))\n",
    "print(getMinimumCost(3, [2, 5, 6]))\n",
    "print(getMinimumCost(2, [2, 5, 6]))\n",
    "print(getMinimumCost(3, [1, 3, 5, 7, 9]))"
   ]
