    "def isHappy(n):\n",
    "    seen = set()\n",
    "    m = n\n",
    "    for _ in range(10):\n",
    "        res = 0\n",
    "        while m > 0:\n",
    "            res = res + (m%10)**2\n",
    "            m = m//10\n",
    "        print(res)\n",
    "        m = res\n",
    "        seen.add(res)\n",
    "    print(seen)"
   ]
